<% include header %>


   <% if(rs.length > 0) { %>
    <table>
	 <tr><th>Username</th></tr>
        <% for (var i=0; rs.length > i; i++) { %>
            <tr>
             <td><a href="/user/E?AccountID=<%= rs[i].AccountID %>"> <%= rs[i].Username %></a></td>
            </tr>
        <% } %>
    </table>
    <% } else { %>
         No users are entered into the database.
    <% } %>

