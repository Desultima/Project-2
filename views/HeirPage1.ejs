<% include header %>
	Choose the Wealthy Person To Add children to.
	<% if ( rs.length > 0) { %>
	<form method="get" action="/user/Heir2">
		<label for="PersonID">Users</label>
		<select id="PersonID" name="PersonID">
			<%for (var i=0; rs.length > i; i++){ %>
			<option value="<%=rs[i].PersonID %>">
				<%= rs[i].FirstName %>
               	<%= rs[i].LastName %>

			</option>
			<% } %>
		</select>
		<input type="submit" >
		</form>
		<% } else { %>
			No users are entered into the database.
		<% } %>
<% include footer %>
